@model SrgDomain.Model.TaskHistory
@{
    ViewData["Title"] = "Task History Details";
}
<h2>Task History Details</h2>
<div>
    <h4>Task History</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Task</dt>
        <dd class="col-sm-10">
            <a asp-controller="Tasks" asp-action="Details" asp-route-id="@Model.TaskId">
                Task @Model.TaskId
            </a>
        </dd>
        <dt class="col-sm-2">@Html.DisplayNameFor(model => model.Action)</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.Action)</dd>
        <dt class="col-sm-2">@Html.DisplayNameFor(model => model.Timestamp)</dt>
        <dd class="col-sm-10">@Model.Timestamp.ToString("g")</dd>
    </dl>
</div>
<p>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index">Back to List</a> |
    <a asp-controller="Tasks" asp-action="Details" asp-route-id="@Model.TaskId">Back to Task</a>
</p>
